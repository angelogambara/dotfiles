{
  "extractor": {
    "base-directory": "~/gallery-dl",
    "cookies": [
      "brave+gnomekeyring"
    ],
    "exhentai": {
      "directory": [
        "{category}",
        "g",
        "{gid}",
        "{token}"
      ],
      "filename": "{gid}_{num:>04}_{image_token}_{filename}.{extension}"
    },
    "fanbox": {
      "directory": [
        "{category}",
        "posts",
        "@{creatorId}"
      ],
      "filename": "{id}_p{num}.{extension}"
    },
    "pixiv": {
      "directory": [
        "{category}",
        "users",
        "{user[id]}"
      ],
      "favorite": {
        "directory": [
          "{category}",
          "users",
          "{user_bookmark[id]}",
          "bookmarks",
          "artworks",
          "{user[id]}"
        ]
      },
      "filename": "{id}_p{num}.{extension}"
    },
    "patreon": {
      "directory": [
        "{category}",
        "c",
        "{creator[vanity]}"
      ],
      "filename": "[{date:%Y.%m.%d}] {title}.{extension}"
    },
    "twitter": {
      "likes": {
        "directory": [
          "{category}",
          "likes",
          "{user[name]}"
        ]
      },
      "media": {
        "directory": [
          "{category}",
          "media",
          "{user[name]}"
        ]
      },
      "filename": "{tweet_id}_{num}.{extension}"
    }
  }
}
